{{template "base" .}}

{{define "content"}}
<div class="container">
    <div class="row">
        <div class="col">
            <h1 class="mt-3">Choose Room</h1>
            <hr>
            {{$rooms := index .Data "rooms"}}
            {{range $rooms}}
                {{.RoomName}}
            {{end}}
        </div>
    </div>
</div>
{{end}}